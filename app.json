{
  "expo": {
    "name": "mirrora",
    "slug": "mirrora",
    "version": "1.0.0",
    "scheme": "mirrora",
    "orientation": "portrait",
    "icon": "./assets/images/companylogo.png",
    "userInterfaceStyle": "light",
    "newArchEnabled": true,
    "splash": {
      "image": "./assets/images/companylogo.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff",
      "hideAppLoadingScreen": true
    },
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/companylogo.png",
        "backgroundColor": "#ffffff"
      },
      "edgeToEdgeEnabled": true,
      "package": "com.mirrora.app"
    },
    "web": {
      "favicon": "./assets/favicon.png"
    },
    "plugins": ["expo-router"]
  }
}

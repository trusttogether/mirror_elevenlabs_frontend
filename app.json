{
  "expo": {
    "name": "mirrora",
    "slug": "mirrora",
    "version": "1.0.0",
    "scheme": "mirrora",
    "orientation": "portrait",
    "icon": "./assets/images/companylogo.png",
    "userInterfaceStyle": "light",
    "newArchEnabled": true,
    "splash": {
      "image": "./assets/images/companylogo.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff",
      "hideAppLoadingScreen": true
    },
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/companylogo.png",
        "backgroundColor": "#ffffff"
      },
      "edgeToEdgeEnabled": true,
      "package": "com.mirrora.app"
    },
    "web": {
      "favicon": "./assets/favicon.png"
    },
    "plugins": [
      "expo-router"
    ],
    "extra": {
      "router": {},
      "eas": {
        "projectId": "fac94821-f63f-4bad-8c6b-9bd53e2783b1"
      }
    },
    "runtimeVersion": {
      "policy": "appVersion"
    },
    "updates": {
      "url": "https://u.expo.dev/fac94821-f63f-4bad-8c6b-9bd53e2783b1"
    }
  }
}
